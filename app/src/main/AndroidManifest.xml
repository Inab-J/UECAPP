<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.inab_j.uecapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".view.activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.activity.CalendarActivity"
            android:label="@string/title_activity_calendar"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.activity.CancelActivity"
            android:label="cancel"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.activity.TimeTableActivity"
            android:label="@string/title_activity_time_table"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.activity.LibraryActivity"
            android:label="@string/title_activity_library"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.activity.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.activity.LinkActivity"
            android:label="@string/title_activity_link"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.activity.MyTimeTableActivity"
            android:label="@string/title_activity_my_time_table"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>
